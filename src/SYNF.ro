        S ZT(17)="skinTests"
        S ZT(18)="exams"
        S ZT(19)="education"
        S ZT(20)="insurance"
        K DIR
        S DIR(0)="SO^"
        F ZI=1:1:20 S DIR(0)=DIR(0)_ZI_":"_ZT(ZI)_";"
        S DIR("B")=1
