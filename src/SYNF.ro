wsVPR(VPR,FILTER)       ; get from web service call
        I '$D(DT) N DIQUIET S DIQUIET=1 D DT^DICRW
        N ZDFN,ZTYPE
        ;N ZTMP
        S ZDFN=$G(FILTER("patientId"))
        I ZDFN="" S ZDFN=$G(FILTER("patientID"))
