        . S ZDFN=$O(^DPT("AFICN",ICN,""))
        I ZDFN="" D  ; try ien
        . N IEN S IEN=$G(FILTER("ien"))
        . I IEN="" Q  ;
        . S ZDFN=$$ien2dfn^SYNFUTL(IEN)
        I ZDFN="" S ZDFN=2
        S ZTYPE=$G(FILTER("domain"),"all")
