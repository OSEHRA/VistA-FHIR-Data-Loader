adb(rtn,ary,pred,obj)   ; returns the array defined by the predicate and object
        if '$d(@ary@("value")) do valueDex(ary,"value")
        new ref
        set ref=@ary@("value",obj) 
        if ref'[pred quit  ;
        do replace(.ref,","""_pred_"""","")
        ;w !,ref b
        merge @rtn=@ref
